    "keywords": ["magicproperties", "properties", "getter", "setter", "accessor", "mutator", "trait"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/maxalmonte14/magicproperties/issues",
        "source": "https://github.com/maxalmonte14/magicproperties"
    },
    "authors": [
        {
            "name": "Max Almonte",
            "email": "maxalmonte14@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MagicProperties\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
